{
    "name": "ghanem/laravel-zoom",
    "version": "1.3.0",
    "description": "A comprehensive Zoom integration package for Laravel, providing easy-to-use API functionality to interact with the Zoom platform using PHP.",
    "homepage": "https://github.com/AbdullahGhanem/laravel-zoom",
    "keywords": [
        "laravel-zoom",
        "zoom",
        "laravel",
        "api",
        "client"
    ],
    "type": "library",
    "require": {
        "guzzlehttp/guzzle": "^7.6",
        "php": "^7.2|^8.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Ghanem\\Zoom\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Abdullah Ghanem",
            "email": "3bdullah.ghanem@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Ghanem\\Zoom\\ZoomServiceProvider"
            ],
            "aliases": {
                "Zoom": "Ghanem\\Zoom\\Facades\\Zoom"
            }
        }
    }
}
